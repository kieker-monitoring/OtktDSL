<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="otkt" nsURI="http://www.otel.kieker/translation/Otkt" nsPrefix="otkt">
  <eClassifiers xsi:type="ecore:EClass" name="Model">
    <eStructuralFeatures xsi:type="ecore:EReference" name="main" eType="#//Main" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Main">
    <eStructuralFeatures xsi:type="ecore:EReference" name="otelSpans" upperBound="-1"
        eType="#//OtelSpan" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="record" upperBound="-1"
        eType="#//KiekerMonitoringType" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mappings" upperBound="-1"
        eType="#//Mapping" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="collector" eType="#//Collector"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Mapping">
    <eStructuralFeatures xsi:type="ecore:EReference" name="from" eType="#//OtelSpan"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DefaultMapping" eSuperTypes="#//Mapping">
    <eStructuralFeatures xsi:type="ecore:EReference" name="to" eType="ecore:EClass ../KiekerRecord.ecore#//KiekerRecord"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OtelSpan">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="defaultAttributes" upperBound="-1"
        eType="#//DefaultAttributes" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="attributes" upperBound="-1"
        eType="#//SpanAttribute" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DefaultAttributes">
    <eStructuralFeatures xsi:type="ecore:EReference" name="trace" eType="#//SpanAttribute"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parentTrace" eType="#//SpanAttribute"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="spanId" eType="#//SpanAttribute"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="startTime" eType="#//SpanAttribute"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="endTime" eType="#//SpanAttribute"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="KiekerMonitoringType"/>
  <eClassifiers xsi:type="ecore:EClass" name="DefaultMonitoringRecord" eSuperTypes="#//KiekerMonitoringType">
    <eStructuralFeatures xsi:type="ecore:EReference" name="name" eType="ecore:EClass ../KiekerRecord.ecore#//KiekerRecord"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NewRecord" eSuperTypes="#//KiekerMonitoringType">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//RecordType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="attributes" upperBound="-1"
        eType="#//RecordAttribute" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="RecordType">
    <eLiterals name="Flow" literal="flow"/>
    <eLiterals name="Normal" value="1" literal="normal"/>
    <eLiterals name="Beforeafter" value="2" literal="beforeafter"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RecordAttribute">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//Type"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SpanAttribute">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//Type"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="annotation" eType="#//Annotation"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="dependency" eType="#//Dependency"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Dependency">
    <eLiterals name="DEFAULT" literal="default"/>
    <eLiterals name="Global" value="1" literal="global"/>
    <eLiterals name="Parent" value="2" literal="parent"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Annotation">
    <eLiterals name="DEFAULT" literal="default"/>
    <eLiterals name="INC" value="1" literal="inc"/>
    <eLiterals name="DEC" value="2" literal="dec"/>
    <eLiterals name="MULT" value="3" literal="mult"/>
    <eLiterals name="DIV" value="4" literal="divide"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CustomMapping" eSuperTypes="#//Mapping">
    <eStructuralFeatures xsi:type="ecore:EReference" name="to" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mappingRules" upperBound="-1"
        eType="#//MappingRule" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MappingRule">
    <eStructuralFeatures xsi:type="ecore:EReference" name="leftSide" eType="#//OtelRef"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="rightSide" eType="#//KiekerReference"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Type">
    <eLiterals name="Default" literal="default"/>
    <eLiterals name="Int" value="1" literal="int"/>
    <eLiterals name="String" value="2" literal="string"/>
    <eLiterals name="Long" value="3" literal="long"/>
    <eLiterals name="Float" value="4" literal="float"/>
    <eLiterals name="Double" value="5" literal="double"/>
    <eLiterals name="Bool" value="6" literal="bool"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OtelRef">
    <eStructuralFeatures xsi:type="ecore:EReference" name="refEntity" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refAttribute" eType="#//SpanAttribute"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="KiekerReference">
    <eStructuralFeatures xsi:type="ecore:EReference" name="refEntity" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refAttribute" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Collector">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="port" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="hostname" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
